<gateway name="filter-test">
   <header xmlns:dc="http://purl.org/dc/elements/1.1/">
      <dc:title>GIAL test filter</dc:title>
      <dc:contributor>Joan Spanne</dc:contributor>
      <dc:date>2009-04-26</dc:date>
      <dc:description>This filter for GIAL record set.</dc:description>
   </header>
   <select-stage>
      <!--
         The GIAL records use a 590 to specify the ISO 639-3 language
         that is the focus of the work (whether its subject or its 
         minority language of content)
         590  ..	‡lmbd‡2ISO 639-3¶
         
      -->
      <test>
         <data-field test="exists" tag="590" code="l"/>
      </test>
      <test>
         <data-field test="contains" tag="24x" code="">
            <text>Dictionar</text>
            <text>dictionar</text>
            <text>Lexic</text>
            <text>lexic</text>
            <text>Vocabular</text>
            <text>vocabular</text>
            <text>Linguistic</text>
            <text>linguistic</text>
            <text>Gramma</text>
            <text>gramma</text>
            <text>Phonolog</text>
            <text>phonolog</text>
         </data-field>
      </test>
      <test>
         <data-field test="contains" tag="650" code="">
            <text>language</text>
         </data-field>
      </test>
   </select-stage>
   
   <reject-stage>
