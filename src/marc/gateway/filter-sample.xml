<?oxygen RNGSchema="file:gateway.rnc" type="compact"?>
<gateway name="filter-test">
   <header xmlns:dc="http://purl.org/dc/elements/1.1/">
      <dc:title>Simple test filter</dc:title>
      <dc:contributor>Gary Simons</dc:contributor>
      <dc:date>2009-02-03</dc:date>
      <dc:description>This is the minimal filter that accepts
         everything and rejects nothing.</dc:description>
   </header>
   <select-stage>
      <leader position="1" length="3" test="equals">012</leader>
<!--
      <control-field position="7" length="1" test="equals" tag="008">s</control-field>
  -->
      <data-field test="contains" tag="xxx" code="">morphology</data-field>
      <data-field test="contains" tag="245" code="ab">dictionary</data-field>
      <data-field test="contains" tag="24x" code="">lexicon</data-field>
      <data-field test="exists" tag="246" code=""></data-field>

   </select-stage>
   <reject-stage>
      <leader position="1" length="3" test="equals">012</leader>
   </reject-stage>
   <crosswalk/>
</gateway>