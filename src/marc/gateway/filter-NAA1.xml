<?xml version="1.0" encoding="UTF-8"?>
<?oxygen RNGSchema="file:gateway.rnc" type="compact"?>
<gateway name="filter-test">
   <header xmlns:dc="http://purl.org/dc/elements/1.1/">
      <dc:title>Simple test filter</dc:title>
      <dc:contributor>Joan Spanne</dc:contributor>
      <dc:date>2009-04-19</dc:date>
      <dc:description>This filter for NAA record set mostly to remove unwanted image records.</dc:description>
   </header>
   <select-stage>
      <!--
         The NAA record set contains a 546 Language Note or a 695 locally defined subject term
         noting language or ethnolinguistic group information for most desired records.
         The set also contains numerous graphics of little or no interest. However, they
         do not use the leader to indicate record type.
      -->
      <test>
         <data-field test="exists" tag="546" code=""/>
      </test>
      <test>
         <data-field test="exists" tag="695" code=""/>
      </test>
      <test>
         <data-field test="contains" tag="24x" code="">
            <text>dictionar</text>
            <text>lexicon</text>
            <text>lexical</text>
            <text>language</text>
            <text>vocabular</text>
            <text>linguistic</text>
            <text>grammatical</text>
            <text>phonolog</text>
         </data-field>
      </test>
      <test>
         <data-field test="contains" tag="520" code="">
            <text>dictionar</text>
            <text>lexicon</text>
            <text>lexical</text>
            <text>language</text>
            <text>vocabular</text>
            <text>linguistic</text>
            <text>grammatical</text>
            <text>phonolog</text>
         </data-field>
      </test>
   </select-stage>
   
   <reject-stage>
      <test>
         <data-field test="contains" tag="245" code="">
            <text>painting</text>
            <text>drawing</text>
            <text>photograph</text>
         </data-field>
      </test>
      <test>
         <data-field test="contains" tag="300" code="">
            <text>photograph</text>
            <text>picture</text>
         </data-field>
      </test>
      <test>
         <data-field test="contains" tag="593" code="">
            <text>photo</text>
            <text>picture</text>
         </data-field>
      </test>
      <test>
         <data-field test="contains" tag="596" code="">
            <text>photograph</text>
            <text>picture</text>
         </data-field>
      </test>
      <test>
         <data-field test="contains" tag="673" code="">
            <text>photograph</text>
            <text>picture</text>
         </data-field>
      </test>
   </reject-stage>
   <crosswalk/>
</gateway>